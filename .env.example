# Environment variables for Go Init Program builds
# Copy this file to .env and customize as needed

# Version information (will be embedded in binary)
VERSION=dev
COMMIT_SHA=unknown
BUILD_TIME=unknown

# Build configuration
GOOS=linux
GOARCH=amd64
CGO_ENABLED=1

# Docker configuration
DOCKER_REGISTRY=ghcr.io
DOCKER_NAMESPACE=therealsibasishhehera
IMAGE_NAME=init-go
IMAGE_TAG=latest

# Container runtime settings
LOG_LEVEL=info
APP_ENV=production

# CI/CD settings
GITHUB_TOKEN=
CODECOV_TOKEN=

# Build optimization
COMPRESS_BINARY=false
STATIC_BUILD=true
DEBUG_BUILD=false

# Testing configuration
TEST_MODE=0
STRESS_TEST=0
SIGNAL_TEST=0